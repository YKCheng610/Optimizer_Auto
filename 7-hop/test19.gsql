create or replace query test19() for graph ldbc_snb syntax v2 {
A = SELECT p1
FROM Tag:t1 - (HAS_TAG_REVERSE>) - Comment:m1 - (HAS_CREATOR>) - Person:p1
WHERE p1.gender == "male";

B = SELECT p2
FROM Person:p2 - (<HAS_MODERATOR) - Forum:f1
WHERE p2.gender == "female";

C = SELECT u1
FROM B:p2 - (STUDY_AT>) - University:u1 - (<STUDY_AT) - A:p1
WHERE p1.browserUsed == p2.browserUsed;

D = SELECT c1
FROM C:u1 - (IS_LOCATED_IN>) - City:c1 - (IS_LOCATED_IN_REVERSE>) - B:p2
WHERE c1.name LIKE "+[^GECM]+" and c1.id <= 200;
}